{
  "name": "@eth-optimism/chains",
  "version": "0.0.1",
  "description": "Chain constants for the OP Stack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethereum-optimism/optimism.git",
    "directory": "packages/chains"
  },
  "homepage": "https://optimism.io",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "src/index.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "preinstall": "npx only-allow pnpm",
    "generate": "tsx scripts/chains/generate.js && tsup && pnpm lint:fix",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@eth-optimism/superchain-registry": "github:ethereum-optimism/superchain-registry",
    "@types/change-case": "^2.3.1",
    "change-case": "5.4.2",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "yaml": "^2.3.4"
  },
  "dependencies": {
    "viem": "^2.1.1"
  }
}
